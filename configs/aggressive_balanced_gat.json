{
    "epochs": 50,
    "batch_size": 16,
    "ID_model": "aggressive_balanced_gat",
    "use_adamW": true,
    "model_type": "gat",
    "use_graphnorm": true,
    "feature_selection": "target",
    "graphs_path": "output/graphs_target_",
    "hidden_channels": 256,
    "dropout_rate": 0.4,
    "lr": 0.0005,
    "loss_weight": false,
    "weight_decay": 1e-5,
    "heads": 8,
    "use_third_layer": true,
    "early_stopping": true,
    "use_balanced_sampling": true,
    "use_focal_loss": true,
    "focal_alpha": 2,
    "focal_gamma": 3,
    "use_data_balancing": true,
    "target_samples_per_class": 3000,
    "max_oversampling_ratio": 4.0
} 