{
    "study_name": "gat_balanced",
    "direction": "maximize",
    "metric_to_optimize": "f1_score",
    "n_trials": 20,
  
    "fixed_params": {
      "epochs": 50,
      "batch_size": 32,
      "model_type": "gat",
      "feature_selection": "target",
      "graphs_path": "graphs_target_",
      "use_graphnorm": true,
      "use_adamW": true,
      "early_stopping": false,
      "use_balanced_sampling": true,
      "use_data_balancing": true,
      "use_third_layer": true,
      "use_focal_loss": true
    },
  
    "hyperparameters": {
      "lr": {
        "name": "lr",
        "type": "float",
        "low": 1e-5,
        "high": 1e-2,
        "log": true
      },
      "hidden_channels": {
        "name": "hidden_channels",
        "type": "categorical",
        "choices": [32, 64, 128, 256]
      },
      "dropout_rate": {
        "name": "dropout_rate",
        "type": "float",
        "low": 0.1,
        "high": 0.5
      },
      "weight_decay": {
        "name": "weight_decay",
        "type": "float",
        "low": 1e-6,
        "high": 1e-3,
        "log": true
      },
      "heads": {
        "name": "heads",
        "type": "categorical",
        "choices": [1, 2, 4, 8, 16]
      },
      "loss_weight": {
        "name": "loss_weight",
        "type": "categorical",
        "choices": [true, false]
      },
      "focal_alpha": {
        "name": "focal_alpha",
        "type": "float",
        "low": 0.5,
        "high": 2.0
      },
      "focal_gamma": {
        "name": "focal_gamma",
        "type": "float",
        "low": 1.0,
        "high": 4.0
      },
      "target_samples_per_class": {
        "name": "target_samples_per_class",
        "type": "categorical",
        "choices": [null, 1000, 2000, 3000, 5000]
      },
      "max_oversampling_ratio": {
        "name": "max_oversampling_ratio",
        "type": "float",
        "low": 1.5,
        "high": 5.0
      }
    }
  }
  